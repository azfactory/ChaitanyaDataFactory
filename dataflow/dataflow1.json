{
	"name": "dataflow1",
	"properties": {
		"folder": {
			"name": "Learning"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_Learning_Storage",
						"type": "DatasetReference"
					},
					"name": "EmployeeData"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "split1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          EmployeeID as short,",
				"          FirstName as string,",
				"          LastName as string,",
				"          EMail as string,",
				"          Phone as string,",
				"          HireDate as string,",
				"          Salary as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     ignoreNoFilesFound: false) ~> EmployeeData",
				"EmployeeData split(isNull(toDate(HireDate,'yyyy-MM-dd')),",
				"     disjoint: false) ~> split1@(Errorrows, Goodrows)"
			]
		}
	}
}