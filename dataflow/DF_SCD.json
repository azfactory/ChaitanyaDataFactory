{
	"name": "DF_SCD",
	"properties": {
		"description": "Slowly Change Dimension ",
		"folder": {
			"name": "Learning"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_EMPLOYEE",
						"type": "DatasetReference"
					},
					"name": "EmployeeList"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          EmployeeID as integer,",
				"          FirstName as string,",
				"          LastName as string,",
				"          EMail as string,",
				"          Phone as string,",
				"          HireDate as string,",
				"          Salary as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     enableCdc: true,",
				"     mode: 'read',",
				"     skipInitialLoad: true) ~> EmployeeList"
			]
		}
	}
}